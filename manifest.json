{
    "name": "Drink Water",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "notifications",
        "storage",
        "alarms"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icons/cb16.png",
        "48": "icons/cb48.png",
        "128": "icons/cb128.png"
      }
    }
}